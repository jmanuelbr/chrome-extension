{
  "name": "chrome-extension",
  "version": "1.0.0",
  "scripts": {
    "build": "node_modules/.bin/webpack --mode=development",
    "prod": "node_modules/.bin/webpack -p",
    "watch": "node_modules/.bin/webpack --watch --mode=development",
    "start": "node_modules/.bin/webpack --mode=development ./src/js/content.js -o ./build/js/content.js",
    "report": "webpack-bundle-analyzer --port 8185 build/js/stats.json"
  },
