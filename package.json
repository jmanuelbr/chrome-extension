    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/preset-env": "^7.12.16",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
